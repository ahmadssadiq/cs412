{
    "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
    "functions": {
        "cs412/wsgi.py": {
            "runtime": "python3.9"
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "cs412/wsgi.py"
        }
    ]
}