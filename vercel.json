{
    "builds": [
        {
            "src": "api/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "buildCommand": "pip install -r ./requirements.txt && python manage.py collectstatic --noinput",
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfile/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/wsgi.py"
        }
    ]
}